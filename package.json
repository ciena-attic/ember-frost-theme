{
  "name": "ember-frost-theme",
  "version": "1.4.6",
  "description": "The default blueprint for ember-cli addons.",
  "frostGuideDirectory": [
    {
      "palette": "design-conventions/colors"
    },
    {
      "typography": "design-conventions/typography"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "lint": "eslint *.js tests",
    "test": "npm run lint && ember test"
  },
  "repository": "git@github.com:ciena-frost/ember-frost-theme.git",
  "engines": {
    "node": ">= 5.0.0"
  },
  "author": "Steven Glanzer (https://github.com/sglanzer)",
  "contributors": [
    "Adam Meadows (https://github.com/job13er)",
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-cli": "2.4.2",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-babel": "^5.1.5",
    "ember-cli-blanket": "0.9.1",
    "ember-cli-content-security-policy": "0.5.0",
    "ember-cli-dependency-checker": "^1.1.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-ic-ajax": "0.2.4",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-mocha": "0.10.1",
    "ember-cli-release": "0.2.8",
    "ember-cli-sass": "^5.2.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-data": "^2.4.0",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-load-initializers": "^0.5.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "^0.2.0",
    "eslint": "2.2.0",
    "eslint-config-frost-standard": "^1.0.0",
    "loader.js": "^4.0.0",
    "sass-lint": "^1.5.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
